#include<stdio.h>
int main()
{ 
  int a[10]; //declaring the array 
  
  printf("Enter the values of the array: \n"); //taking user input 
  
  for(int i=0;i<=9;i++)
  { 
   scanf("%d",&a[i]);
  }
  
  printf("The reversed array is: \n");
  
  for(int i=9;i>0;i--) //printing the array backwards 
  {
  	printf("%d \n",a[i]);
  }
  
  return 0;
} 

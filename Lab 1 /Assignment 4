#include<stdio.h>
int main()
{
	int f,c; //reading the farenheit and celsius variables 
	printf("Enter the Temperature in Farenheit:");
	scanf("%d",&f); // Assigning user input value to the farenheit variable 
	
	c= (f-32)*5/9; //Celsius to farenheit formula 
	
	printf("The given temperature is: %dC",c);
	return 0;
	
}

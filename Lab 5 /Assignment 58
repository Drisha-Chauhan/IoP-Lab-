#include<stdio.h>
int main()
{
	int a[10],b[10],c[10],i;
	
	printf("Enter the values of the first array: \n");
	
	for (i=0;i<10;i++)
	{
		scanf("%d",&a[i]);
	}
	
	printf("Enter the values of the second array: \n");
	
	for (i=0;i<10;i++)
	{
		scanf("%d",&b[i]);
	}
	
	for (i=0;i<10;i++)
	{
		c[i]= a[i]+b[i];
    }
    
    printf("The third array: ");
    for (i=0;i<10;i++)
	{
	   	printf("%d \n",c[i]);
	}
	
	
}

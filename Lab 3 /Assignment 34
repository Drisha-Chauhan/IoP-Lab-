#include<stdio.h>
int main()
{
	int a,b,c,i; //Declaring the variables 
	
	a=0; //Fixing the intial values of the fibonacci series 
	b=1;
	
	printf("%d ",a); //Displaying the intial values 
	printf("%d ",b);
	
	for(i=1;i<=10;i++) //'For' loop for the following values of the series
	{
		c=a+b;
		
		printf("%d ",c);
		
		a=b;
		b=c;
	}
	
	return 0;
}
